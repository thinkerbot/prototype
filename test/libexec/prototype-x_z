#!/bin/bash
#############################################################################
prototype_x_z_usage () {
printf "%s" "\
usage: prototype x z [-h]

  Test x-z.  More documentation.

options:

  -h  prints help

"
}
while getopts "h" option
do
  case $option in
    (h)  prototype_x_z_usage
         exit 0 ;;
    (*)  prototype_x_z_usage | head -n 1
         exit 2 ;;
  esac
done
#############################################################################

printf "x-z\n"
