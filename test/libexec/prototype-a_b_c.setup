#!/bin/bash
prototype_setup_a_b_c_usage () {
printf "%s" "\
usage: prototype setup a b c [OPTIONS]

  Setup a_b_c.

options:

  -h          prints help

"
}
while getopts "h" option
do
  case $option in
    (h)  prototype_setup_a_b_c_usage
         exit 0 ;;
    (*)  prototype_setup_a_b_c_usage | head -n 2
         exit 2 ;;
  esac
done
shift $(($OPTIND - 1))
######################################
export PROTOTYPE_TEST_A_B_C=a_b_c.setup

mkdir -p "${PROTOTYPE_SETUP_DIR}"
prototype_test_env > "${PROTOTYPE_SETUP_DIR}/c"
