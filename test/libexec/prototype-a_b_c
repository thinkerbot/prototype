#!/bin/bash
#############################################################################
prototype_a_b_c_usage () {
printf "%s" "\
usage: prototype a b c [-h]

  Test a-b-c.  More documentation.

options:

  -h  prints help
"
}
while getopts "h" option
do
  case $option in
    (h)  prototype_a_b_c_usage
         exit 0 ;;
    (*)  prototype_a_b_c_usage | head -n 1
         exit 2 ;;
  esac
done
#############################################################################
export PROTOTYPE_TEST_A_B_C=a_b_c

prototype_test_env
